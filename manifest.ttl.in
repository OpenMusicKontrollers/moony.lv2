# Common UI
moony:ui1_common_ui
	a ui:UI ;
	ui:binary <moony_ui@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <moony.ttl> .
moony:ui3_common_kx
	a kx:Widget ;
	ui:binary <moony_ui@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <moony.ttl> .
moony:ui5_common_x11
	a ui:X11UI ;
	ui:binary <moony_ui@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <moony.ttl> .
moony:ui6_common_eo
	a ui:EoUI ;
	ui:binary <moony_ui@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <moony.ttl> .

# Simple UI
